{
  "dashboard": {
    "title": "Data Lineage & Versioning",
    "tags": ["lineage", "versioning", "iceberg", "janusgraph"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",

    "panels": [
      {
        "id": 1,
        "title": "Dataset Versions by Namespace",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "sum by (namespace) (iceberg_snapshots_total)",
            "legendFormat": "{{namespace}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },

      {
        "id": 2,
        "title": "Pipeline Executions (Last 24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT COUNT(*) as count FROM pipeline_executions WHERE start_time >= NOW() - INTERVAL '24 hours'",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },

      {
        "id": 3,
        "title": "Model Training Events",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT COUNT(*) as count FROM model_cards WHERE training_timestamp >= NOW() - INTERVAL '24 hours'",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            }
          }
        }
      },

      {
        "id": 4,
        "title": "Pipeline Success Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 4},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT (COUNT(*) FILTER (WHERE status = 'SUCCESS')::float / NULLIF(COUNT(*), 0)) * 100 as success_rate FROM pipeline_executions WHERE start_time >= NOW() - INTERVAL '24 hours'",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },

      {
        "id": 5,
        "title": "JanusGraph Vertices (Lineage Nodes)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 4},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "janusgraph_vertices_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },

      {
        "id": 6,
        "title": "Iceberg Snapshot Creation Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT time_bucket('1 hour', timestamp) as time, namespace, COUNT(*) as snapshots FROM iceberg_snapshots_log WHERE timestamp >= NOW() - INTERVAL '7 days' GROUP BY 1, 2 ORDER BY 1",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 70
            }
          }
        }
      },

      {
        "id": 7,
        "title": "Lineage Query Performance (P95)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "histogram_quantile(0.95, sum by (le, query_type) (rate(janusgraph_query_duration_seconds_bucket[5m])))",
            "legendFormat": "{{query_type}} (p95)",
            "refId": "A"
          },
          {
            "datasource": "Prometheus",
            "expr": "histogram_quantile(0.99, sum by (le, query_type) (rate(janusgraph_query_duration_seconds_bucket[5m])))",
            "legendFormat": "{{query_type}} (p99)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": ".*p99.*"},
              "properties": [
                {"id": "custom.lineStyle", "value": {"fill": "dash"}}
              ]
            }
          ]
        }
      },

      {
        "id": 8,
        "title": "Top 10 Tables by Snapshot Count",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT namespace || '.' || table_name as table, COUNT(*) as snapshots, MAX(timestamp) as last_snapshot FROM iceberg_snapshots_log GROUP BY 1 ORDER BY 2 DESC LIMIT 10",
            "format": "table",
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {"displayName": "snapshots", "desc": true}
          ]
        }
      },

      {
        "id": 9,
        "title": "Model Lineage Completeness",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT CASE WHEN training_dataset_version IS NOT NULL THEN 'With Lineage' ELSE 'Without Lineage' END as status, COUNT(*) as count FROM model_cards GROUP BY 1",
            "format": "table",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "donut",
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },

      {
        "id": 10,
        "title": "Dataset Size by Layer",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "sum by (namespace) (iceberg_table_size_bytes)",
            "legendFormat": "{{namespace}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "min": 0
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },

      {
        "id": 11,
        "title": "Pipeline Execution Duration (by Pipeline)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT start_time as time, pipeline_id, duration_ms / 1000.0 as duration_seconds FROM pipeline_executions WHERE start_time >= NOW() - INTERVAL '7 days' AND status = 'SUCCESS' ORDER BY start_time",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "points",
              "pointSize": 5
            }
          }
        }
      },

      {
        "id": 12,
        "title": "Lineage Graph Size Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "janusgraph_vertices_total",
            "legendFormat": "Vertices (Nodes)",
            "refId": "A"
          },
          {
            "datasource": "Prometheus",
            "expr": "janusgraph_edges_total",
            "legendFormat": "Edges (Relationships)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            }
          }
        }
      },

      {
        "id": 13,
        "title": "Failed Pipeline Executions (Last 24h)",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT pipeline_id, pipeline_version, start_time, SUBSTRING(error_message, 1, 100) as error FROM pipeline_executions WHERE status = 'FAILED' AND start_time >= NOW() - INTERVAL '24 hours' ORDER BY start_time DESC LIMIT 20",
            "format": "table",
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "error"},
              "properties": [
                {"id": "custom.width", "value": 400}
              ]
            }
          ]
        }
      },

      {
        "id": 14,
        "title": "Iceberg Snapshot Retention Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 40},
        "targets": [
          {
            "datasource": "PostgreSQL",
            "rawSql": "SELECT COUNT(*) as old_snapshots FROM iceberg_snapshots_log WHERE timestamp < NOW() - INTERVAL '90 days'",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "text": {
            "titleSize": 12,
            "valueSize": 30
          }
        }
      },

      {
        "id": 15,
        "title": "Average File Size by Namespace",
        "type": "bargauge",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 40},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "avg by (namespace) (iceberg_file_size_bytes)",
            "legendFormat": "{{namespace}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 10485760, "color": "yellow"},
                {"value": 104857600, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "displayMode": "basic",
          "orientation": "horizontal"
        }
      },

      {
        "id": 16,
        "title": "Tables Needing Compaction",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 40},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "count(iceberg_small_files_count > 10)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },

      {
        "id": 17,
        "title": "Cassandra Cluster Health (JanusGraph Backend)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 40},
        "targets": [
          {
            "datasource": "Prometheus",
            "expr": "count(up{job=\"cassandra\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [
              {
                "type": "value",
                "options": {
                  "3": {"text": "Healthy (3/3)", "color": "green"}
                }
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 2, "color": "yellow"},
                {"value": 3, "color": "green"}
              ]
            }
          }
        }
      }
    ],

    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "query",
          "datasource": "PostgreSQL",
          "query": "SELECT DISTINCT namespace FROM iceberg_snapshots_log",
          "multi": true,
          "includeAll": true,
          "current": {"value": ["$__all"], "text": "All"}
        },
        {
          "name": "pipeline",
          "type": "query",
          "datasource": "PostgreSQL",
          "query": "SELECT DISTINCT pipeline_id FROM pipeline_executions",
          "multi": true,
          "includeAll": true,
          "current": {"value": ["$__all"], "text": "All"}
        }
      ]
    },

    "annotations": {
      "list": [
        {
          "name": "Model Deployments",
          "datasource": "PostgreSQL",
          "enable": true,
          "iconColor": "blue",
          "query": "SELECT training_timestamp as time, CONCAT('Model deployed: ', model_name, ' v', version) as text FROM model_cards WHERE training_timestamp >= $__timeFrom() AND training_timestamp <= $__timeTo()"
        },
        {
          "name": "Pipeline Failures",
          "datasource": "PostgreSQL",
          "enable": true,
          "iconColor": "red",
          "query": "SELECT start_time as time, CONCAT('Pipeline failed: ', pipeline_id) as text FROM pipeline_executions WHERE status = 'FAILED' AND start_time >= $__timeFrom() AND start_time <= $__timeTo()"
        }
      ]
    },

    "time": {
      "from": "now-7d",
      "to": "now"
    },

    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
