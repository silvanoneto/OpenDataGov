  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "targets": [
        {
          "expr": "sum(rate(http_server_request_duration_seconds_count[5m])) by (service_name)",
          "legendFormat": "{{ service_name }}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "title": "Error Rate (%)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "targets": [
        {
          "expr": "sum(rate(http_server_request_duration_seconds_count{http_status_code=~\"5..\"}[5m])) by (service_name) / sum(rate(http_server_request_duration_seconds_count[5m])) by (service_name) * 100",
          "legendFormat": "{{ service_name }}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "max": 100 },
        "overrides": []
      }
    },
    {
      "title": "P95 Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_request_duration_seconds_bucket[5m])) by (le, service_name))",
          "legendFormat": "{{ service_name }}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "Active Spans",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "targets": [
        {
          "expr": "sum(rate(otel_span_export_count[5m])) by (service_name)",
          "legendFormat": "{{ service_name }}"
        }
      ]
    }
  ],
