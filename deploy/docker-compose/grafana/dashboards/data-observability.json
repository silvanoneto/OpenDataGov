      "title": "Data Freshness (seconds since last update)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "targets": [
        {
          "expr": "odg_data_freshness_seconds",
          "legendFormat": "{{ dataset }} ({{ layer }})"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "Row Count (cumulative)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "victoriametrics" },
      "targets": [
        {
          "expr": "sum(rate(odg_data_row_count_total[5m])) by (dataset, layer)",
          "legendFormat": "{{ dataset }} ({{ layer }})"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "rows/s" },
        "overrides": []
      }
    },
